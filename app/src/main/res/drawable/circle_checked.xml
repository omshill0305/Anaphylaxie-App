<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="?attr/colorPrimary" />

            <size
                android:width="20dp"
                android:height="20dp" />

            <padding
                android:left="2dp"
                android:right="2dp"
                android:bottom="2dp"
                android:top="2dp"/>
        </shape>
    </item>

    <item android:width="16dp" android:height="16dp" android:drawable="@drawable/baseline_done_black_18dp" />
</layer-list>