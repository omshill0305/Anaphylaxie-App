<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/emergency" />
            <stroke
                android:width="4dp"
                android:color="@color/colorPrimaryDark" />
            <size
                android:width="@dimen/medium_icon"
                android:height="@dimen/medium_icon" />
            <padding
                android:bottom="@dimen/default_margin"
                android:left="@dimen/default_margin"
                android:right="@dimen/default_margin"
                android:top="@dimen/default_margin" />
        </shape>
    </item>
    <item
        android:width="@dimen/medium_icon"
        android:height="@dimen/medium_icon"
        android:drawable="@drawable/ic_notruf_white" />
</layer-list>