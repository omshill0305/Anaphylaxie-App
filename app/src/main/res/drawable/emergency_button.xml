<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#e42828" />
            <size
                android:width="@android:dimen/app_icon_size"
                android:height="@android:dimen/app_icon_size" />
            <padding
                android:bottom="@dimen/default_margin"
                android:left="@dimen/default_margin"
                android:right="@dimen/default_margin"
                android:top="@dimen/default_margin" />
        </shape>
    </item>
    <item
        android:width="@android:dimen/app_icon_size"
        android:height="@android:dimen/app_icon_size"
        android:drawable="@drawable/ic_notruf_white" />
</layer-list>